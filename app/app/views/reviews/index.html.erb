<h1>Listing reviews</h1>

<table>
  <tr>
    <th>Review</th>
    <th>Business</th>
    <th>Stars</th>
    <th>Text</th>
    <th>Review date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @reviews.each do |review| %>
  <tr>
    <td><%= review.review_id %></td>
    <td><%= review.business_id %></td>
    <td><%= review.stars %></td>
    <td><%= review.text %></td>
    <td><%= review.review_date %></td>
  </tr>
<% end %>
</table>

<br />


